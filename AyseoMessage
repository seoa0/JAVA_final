package fin;

public class AyseoMessage {
    String user1; //보낸사람
    String user2; //받은사람
    String message;

    public AyseoMessage(String user1, String user2, String message) {
        this.user1 = user1;
        this.user2 = user2;
        this.message = message;
    }

    @Override
    public String toString() {
        String str = "[From: " + user1 + "] " + message;
        return str;
    }
}
